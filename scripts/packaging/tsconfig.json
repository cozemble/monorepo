{
  "compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./lib",
		"declaration": true,
		"moduleResolution": "NodeNext",
		"noImplicitAny": false,
		"esModuleInterop": true
  },
  "ts-node": {
	"esm": true
  },
  "include": [
    "./src"
  ]
}

{
  "name": "@cozemble/frontend-cozemble-forms",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch --threshold error",
    "cypress": "./scripts/run-cypress.sh",
    "test": "svelte-check --tsconfig ./tsconfig.json --threshold error"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "main": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.2.0",
    "cypress-real-events": "^1.7.6",
    "postcss": "^8.4.21",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "4.4.3"
  },
  "dependencies": {
    "@cozemble/lang-util": "workspace:*",
    "zod": "^3.22.2"
  }
}

{
  "name": "@cozemble/frontend-ui-blocks",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch --threshold error"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "4.4.3",
    "@cozemble/model-api": "workspace:*",
    "@cozemble/model-assembled": "workspace:*",
    "@cozemble/model-registries": "workspace:*"
  },
  "type": "module",
  "dependencies": {
    "@cozemble/model-core": "workspace:*",
    "@cozemble/ui-filter-editor": "workspace:*",
    "@cozemble/data-filters-core": "workspace:*",
    "@cozemble/data-filters-config": "workspace:*",
    "@cozemble/lang-util": "workspace:*",
    "@cozemble/model-event-sourced": "workspace:*",
    "@cozemble/data-editor-sdk": "workspace:*",
    "@cozemble/backend-tenanted-api-types": "workspace:*",
    "daisyui": "^2.47.0"
  }
}

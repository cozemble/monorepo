{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist/esm",
		"declaration": true,
		"isolatedModules": false,
		"importsNotUsedAsValues": "preserve",
		"noImplicitAny": false,
		"allowImportingTsExtensions": true,
		"allowSyntheticDefaultImports": true
	}
}

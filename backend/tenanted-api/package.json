{
  "name": "@cozemble/backend-tenanted-api",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npx tsc --noEmit -p tsconfig.json",
    "build-npm": "npx tsc -p tsconfig.json",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "start": "ts-node --esm src/index.ts",
    "db:migrate": "dotenv -- bash -c 'npx postgrator --username $PG_ADMIN_USER --password $PG_ADMIN_PASSWORD'",
    "db:migrate-local": "npx postgrator --host 127.0.0.1 --database postgres --username user --password password",
    "dev": "nodemon -x \"npm run buildrun\"",
    "buildrun": "npm run compile && npm run server",
    "build-run-db": "npm run compile && npm run bounce:db",
    "bounce:db": "node dist/cjs/devDb.js",
    "compile": "npx tsc --noEmit -p tsconfig.json",
    "server": "NODE_ENV=development ts-node --esm src/index.ts",
    "clean:postgres": "./scripts/run-clean-postgres.sh && npm run db:migrate-local"
  },
  "devDependencies": {
    "@types/child-process-promise": "^2.2.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.6",
    "@types/sharp": "^0.31.1",
    "axios": "^1.2.3",
    "dotenv-cli": "^7.2.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.20",
    "postgrator-cli": "^6.0.1",
    "testcontainers": "^9.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "undici": "^5.21.0",
    "vite": "4.4.3",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@cozemble/backend-tenanted-api-types": "workspace:*",
    "@cozemble/data-filters-core": "workspace:*",
    "@cozemble/lang-util": "workspace:*",
    "@cozemble/model-api": "workspace:*",
    "@cozemble/model-core": "workspace:*",
    "@cozemble/model-event-sourced": "workspace:*",
    "@cozemble/model-properties-core": "workspace:*",
    "@google-cloud/storage": "^6.9.4",
    "child-process-promise": "^2.2.1",
    "client-oauth2": "^4.3.3",
    "cors": "^2.8.5",
    "detect-libc": "^2.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.0",
    "multer": "1.4.5-lts.1",
    "multer-cloud-storage": "^3.0.0",
    "openai": "^3.2.1",
    "pg": "^8.9.0",
    "sharp": "^0.32.0"
  }
}
